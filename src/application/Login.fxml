<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="650.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Login">
   <children>
      <Button fx:id="login" layoutX="142.0" layoutY="290.0" mnemonicParsing="false" onAction="#handleLogin" prefHeight="42.0" prefWidth="216.0" text="Login" />
      <TextField fx:id="username" layoutX="142.0" layoutY="131.0" prefHeight="25.0" prefWidth="216.0" promptText="User" />
      <TextField fx:id="password" layoutX="142.0" layoutY="181.0" prefHeight="25.0" prefWidth="216.0" promptText="Password" />
      <Button fx:id="registrationEinzel" layoutX="142.0" layoutY="355.0" mnemonicParsing="false" onAction="#handleRegistrationButtonEinzel" prefHeight="42.0" prefWidth="216.0" text="Registrieren für Einzelkunden" />
      <Button fx:id="forgot" layoutX="142.0" layoutY="484.0" mnemonicParsing="false" onAction="#handlePasswordForgotButton" prefHeight="42.0" prefWidth="216.0" text="Passwort vergessen" />
      <Button fx:id="registrationFirma" layoutX="142.0" layoutY="421.0" mnemonicParsing="false" onAction="#handleRegistrationButtonFirma" prefHeight="42.0" prefWidth="216.0" text="Registrieren für Firmenkunden" />
      <Label layoutX="142.0" layoutY="164.0" text="Passwort:" />
      <Label layoutX="142.0" layoutY="114.0" text="Username:" />
      
   </children>
</AnchorPane>
