<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="650.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Login">
   <children>
      <Button fx:id="login" layoutX="142.0" layoutY="304.0" mnemonicParsing="false" onAction="#handleLogin" prefHeight="42.0" prefWidth="216.0" text="Login" />
      <TextField fx:id="username" layoutX="142.0" layoutY="173.0" promptText="User" />
      <TextField fx:id="password" layoutX="142.0" layoutY="223.0" promptText="Password" />
      <Button fx:id="reg" layoutX="142.0" layoutY="366.0" mnemonicParsing="false" onAction="#handleRegistrationButton" prefHeight="42.0" prefWidth="216.0" text="Registrieren" />
      <Button fx:id="forgot" layoutX="142.0" layoutY="422.0" mnemonicParsing="false" onAction="#handlePasswordForgotButton" prefHeight="42.0" prefWidth="216.0" text="Passwort vergessen" />
      
   </children>
</AnchorPane>
