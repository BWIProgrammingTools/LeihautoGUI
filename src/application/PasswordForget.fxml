<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PasswordForgot">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="136.0" prefWidth="130.0" VBox.vgrow="ALWAYS">
      <children>
            <TextField fx:id="username" layoutX="58.0" layoutY="76.0" promptText="User" />
            <TextField fx:id="email" layoutX="58.0" layoutY="117.0" promptText="E-Mail" />
            <TextField fx:id="alter" layoutX="58.0" layoutY="161.0" promptText="Alter" />
            <Button fx:id="forgot" layoutX="24.0" layoutY="200.0" mnemonicParsing="false" onAction="#handlePasswordForgot" prefHeight="42.0" prefWidth="216.0" text="Passwort vergessen" />
      </children>
    </AnchorPane>
  </children>
</VBox>
