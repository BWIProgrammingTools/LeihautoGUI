<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PasswordForgot">
	<children>
		<AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="400.0" prefWidth="429.0" VBox.vgrow="ALWAYS">
			<children>
				<TextField fx:id="username" layoutX="222.0" layoutY="136.0" prefHeight="25.0" prefWidth="216.0" promptText="Username" />
				<TextField fx:id="email" layoutX="222.0" layoutY="171.0" prefHeight="25.0" prefWidth="216.0" promptText="E-Mail" />
				<TextField fx:id="alter" layoutX="223.0" layoutY="206.0" prefHeight="25.0" prefWidth="216.0" promptText="Alter" />
				<Button fx:id="forgot" layoutX="225.0" layoutY="257.0" mnemonicParsing="false" onAction="#handlePasswordForgot" prefHeight="42.0" prefWidth="216.0" text="Passwort vergessen">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Label layoutX="49.0" layoutY="41.0" prefHeight="26.0" prefWidth="285.0" text="Passwort vergessen">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <Label alignment="CENTER_RIGHT" layoutX="38.0" layoutY="210.0" prefHeight="17.0" prefWidth="180.0" text="Alter:" />
            <Label alignment="CENTER_RIGHT" layoutX="38.0" layoutY="175.0" prefHeight="17.0" prefWidth="180.0" text="E-Mailadresse:" />
            <Label alignment="CENTER_RIGHT" layoutX="38.0" layoutY="140.0" prefHeight="17.0" prefWidth="180.0" text="Username:" />
			</children>
		</AnchorPane>
	</children>
</VBox>
